import java.util.*;


public class Main {
    public static void main(String[] args) throws Exception {
        Scanner sc = new Scanner(System.in);
        int count =sc.nextInt();
        System.out.println("データ個数"+count);
        
        
        String data;
        for (int i=0; i<count; i++){
            data = sc.next();
            System.out.println("hello"+ data);
        }
    }
}
